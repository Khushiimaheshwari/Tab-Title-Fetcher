{
  "manifest_version": 3,
  "name": "Tab Title Fetcher",
  "version": "1.0",
  "description": "Fetch and show the title of the current tab",
  "permissions": ["tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions": ["<all_urls>"]
}
